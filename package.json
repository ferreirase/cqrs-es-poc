{
  "name": "transaction-management",
  "version": "0.0.1",
  "description": "POC de Sistema de Gerenciamento de Transações com CQRS + Event Sourcing",
  "scripts": {
    "build": "cross-env NODE_ENV=production && nest build && npm run copy-js-files",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production && node --no-experimental-fetch dist/main",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down --volumes",
    "copy-js-files": "cp -r src/common/workers/*.js dist/common/workers"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "3.6.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.0",
    "@nestjs/microservices": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/typeorm": "^9.0.0",
    "amqp-connection-manager": "^4.1.10",
    "amqplib": "^0.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "mongoose": "^6.7.0",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.8.0",
    "pino-http": "^10.4.0",
    "pino-loki": "^2.5.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "typeorm": "^0.3.10",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "axios": "^1.8.4",
    "source-map-support": "^0.5.21",
    "supabase": "^2.19.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
